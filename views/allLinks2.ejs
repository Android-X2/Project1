<%- include('partials/header') -%>

<header class="dashboard-header top-container">
    <h3><i class="fas fa-soli fa-tree"></i>Yggdrasil</h3>
    <div class="top-container option-container">
        <a href="/profile/<%= user %>">Link to Public Profile</a>
    </div>
</header>
<div class="center">
    <div class="title-container">
        <h6><%= groups?.title %></h6>
    </div>
    <div class="allLinks-container">
        <ul>
            <% for(let j=0; j<links.length; j++){ %>
                <div class="all-links" >
                    <li>
                        <a href="<%= links[j].link %>" target="_blank" class="bg-primary btn btn-primary btn-block btn-rounded"><%= links[j].title %></a>
                    </li> 
                </div> 
            <% } %>
        </ul> 
    </div>
    
</div>


<%- include('partials/footer') -%>