<%- include('partials/header') -%>

<a href="/profile/<%= user %>">Link to Public Profile</a>
<div >
    <ul class="group-container">
        <% for(var i=0; i<group.length; i++) {%> 
            <li class="thing">
                <div class="list-container">
                    <div>
                        <span><%= group[i].title %></span>
                        <ul>
                            <% for(let j=0;j<links.length;j++){ %>
                                <div class="thing">
                                    <% if(group[i]._id.toString() == links[j].group.toString()) { %>
                                        <li>
                                            <a href="<%= links[j].link %>" target="_blank" class="btn btn-primary btn-block"><%= links[j].title %></a>
    
                                        </li>  
                                        <% } %>
                
                                </div>
                                
                            <% } %>
                        </ul>
                    </div>
                   
                </div>
            </li>
        <% } %>
    </ul>
</div>  

<%- include('partials/footer') -%>